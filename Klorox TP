local Players = game.Players:GetPlayers()
local toggled = true
local plr = game.Players.LocalPlayer
local toggled = true


game.StarterGui:SetCore("SendNotification", {
Title = "Klorox Tp"; -- the title (ofc)
Text = "On; Press X to toggle."; -- what the text says (ofc)
Duration = 5; -- how long the notification should in secounds
})

    game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
        if key == "x" then
            toggled = not toggled
            if toggled then
                game.StarterGui:SetCore("SendNotification", {
Title = "Klorox Tp"; -- the title (ofc)
Text = "On"; -- what the text says (ofc)
Duration = 5; -- how long the notification should in secounds
})
else
    game.StarterGui:SetCore("SendNotification", {
Title = "Klorox Tp"; -- the title (ofc)
Text = "Off"; -- what the text says (ofc)
Duration = 5; -- how long the notification should in secounds
})
end
end
end)


function tpme()
    if toggled then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").TeleportPad.CFrame
        wait(.8)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").LivingThings[_G.MainUser].HumanoidRootPart.CFrame
        game:GetService("ReplicatedStorage").SelfHarm:FireServer(1000)
        wait(.5)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").LivingThings[_G.MainUser].HumanoidRootPart.CFrame
    end
end

game.Players.LocalPlayer.CharacterAdded:Connect(function()
    wait(6.5)
    tpme()
end)

tpme()

local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
   vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
   wait(1)
   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)
