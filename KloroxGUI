local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

local X = Material.Load({
	Title = "Klorox Farm",
	Style = 3,
	SizeX = 200,
	SizeY = 305,
	Theme = "Dark",
})

local Y = X.New({
	Title = "Main"
})

local Z = X.New({
	Title = "Extra"
})

local Reload = Z.Button({
    Text = 'Reload Script',
    Callback = function()
        
        end;
})
local Reset = Z.Button({
    Text = 'Reset',
    Callback = function()
        game.Players.LocalPlayer.Character:BreakJoints()
        end;
})


local A = Y.Button({
    Text = "Knock Self",
    Callback = function()
        game:GetService("ReplicatedStorage").SelfHarm:FireServer(10000)
    end
})

local HealB = Y.Button({
    Text = "Heal",
    Callback = function()
        fireclickdetector(game:GetService("Workspace").HealthPad.ClickDetector)
    end
})

local TP = Y.Toggle({
    Text = "Auto-TP",
    Callback = function(Value)
        ToggleTP = Value
    end,
    Enabled = false
})

local TP2 = Y.Button({
    Text = "Start TP",
    Callback = function()
        TpFarm()
    end
})

local GRP = Y.Toggle({
    Text = "Auto-Grip",
    Callback = function(Value)
        ToggleGrip = Value
    end,
    Enabled = false
})

local LoopH = Y.Toggle({
    Text = "Loop Heal",
    Callback = function(Value)
        ToggleHeal = Value
    end,
    Enabled = false
})

local H = Y.TextField({
	Text = "Player Name",
	Callback = function(Value)
		_G.MainUser = Value
		print(_G.MainUser)
	end,
})

local function TpFarm()
    firetouchinterest(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart,game:GetService("Workspace").TeleportPad,0)
    wait(0.2)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").LivingThings[_G.MainUser].HumanoidRootPart.CFrame * CFrame.new(0, 0, -3)
    game:GetService("ReplicatedStorage").SelfHarm:FireServer(10000)
    wait(0.9)
    for var=1,45 do
       wait(.1)
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").LivingThings[_G.MainUser].HumanoidRootPart.CFrame * CFrame.new(0, -2, -2)
      game:GetService("ReplicatedStorage").SelfHarm:FireServer(10000)
    end
end

game.Players.LocalPlayer.CharacterAdded:Connect(function()
    if ToggleTP then
    wait(6.5)
    TpFarm()
    end
end)

local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
   vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
   wait(1)
   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)

while wait(.05) do
if ToggleHeal then
        fireclickdetector(game:GetService("Workspace").HealthPad.ClickDetector)
        wait(.1)
end
if ToggleGrip then
    game:GetService("Players").LocalPlayer.Backpack.CombatClient.Grip:FireServer()
    wait(0.35)
end
end
